source ~/.vhosts/$(hostname -f) || exit 1

echo "### Entering setup-host"
setup-host
echo "### Entering setup-deb"
setup-deb
echo "### Entering setup-etc"
setup-etc
echo "### Entering setup-www"
setup-www

systemd-cat -t hlog echo "$(whoami) $(basename $0) $*"
